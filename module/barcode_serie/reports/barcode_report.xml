<odoo>
	
	<template id="gen_bar_code_serie">
        <t t-call="web.html_container">
			
			
					
	                    <t t-foreach="docs.move_lines" t-as="ml">
	                        <t t-foreach="ml.move_line_ids" t-as="mls">
	                        	<t t-foreach="mls.product_id" t-as="mlss">
		                        	<t t-foreach="mlss.attribute_value_ids" t-as="mlssl">
		                        		<t t-call="web.basic_layout">
																							
													<div class="row">
														<div class="page">	
										           <div class = 'col-xs-6 text-left'> 
												           <span> <span t-esc="docs.company_id.numero_tienda"/> </span> 
												     </div> 
												     <div class = 'col-xs-6 text-right'> 
												           <span> <span t-esc="time.strftime('%m/%d/%Y',time.strptime(docs.scheduled_date,'%Y-%m-%d %H:%M:%S'))" style="text-align:right;"/></span> 
												     </div> 
												     <br/><br/>
												     <div class = 'col-xs-8' style="font-size:20px; font-weight: bold;"> 
												           <span> 
														        <span >

								                        		<t t-esc="mlss.collection_id.name" /><br/>
								                        		
								                        		<t t-esc="ml.product_id.name" />  <img t-attf-src="data:image/*;base64,{{mlss.image_medium}}" style="width: 20%;"/>
								                        		<br/>
								                        		<t t-esc="mlss.default_code" /> 
								                        		</span>
						                        		    </span> 
						                        		   
												     </div> 
												     
												     <div class = 'col-xs-4 text-center'> 
												     	<br/>
												           <span > 

												           	<div style="
														    transform:rotate(90deg);
															-ms-transform:rotate(90deg); /* IE 9 */
															-webkit-transform:rotate(90deg); /* Opera, Chrome, and Safari */
															position:absolute;
														    font-size:10px;
														   ">

														    	<span>
																	<img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128',mls.product_id.barcode, 120, 30)" /><br/>
																	
																</span>
																<span>
																	<t t-esc="mlss.barcode" />
																</span>
											    			</div>

												            </span> 
												     </div>
												    <!--  <div class = 'col-xs-12 text-center'> 
												           <span> 

												           <img t-attf-src="data:image/*;base64,{{mlss.image_medium}}" style="width: 10%;"/>
														
													    </span> 
													</div> -->
												      <div class = 'col-xs-12 text-center'> 
												           <span> 
												           	Acabado: <t t-esc="mlss.caja"/>
														
													    </span>
														
														
												       
												     </div>
												     <div class = 'col-xs-7 text-right'> 
												           <span> 

												           	<img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128',mls.lot_name, 350, 100)" />
														
													    </span>
														
														<br/>
														<span >
															<t t-esc="mls.lot_name" />
															
														</span>
												       
												     </div> 
												     <div class = 'col-xs-5 text-center'> 
												           <span>
													           <span style="font-size:80px; font-weight: bold;" class="text-right">
															    <t t-esc="mlssl.name"/>
														       </span>
												           </span> 
												     </div> 
												     </div>
										
										</div>
									</t>
								</t>
							</t>
						</t>
					
				
			</t>
		</t>
	</template>
		
	<record id="paperformat_barcode" model="report.paperformat"> 
            <field name="name">Codigo de barras</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">51</field>
            <field name="page_width">76</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">2</field>
            <field name="margin_bottom">2</field>
            <field name="margin_left">1</field>
            <field name="margin_right">1</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">1</field>
            <field name="dpi">130</field>
    </record>

</odoo>